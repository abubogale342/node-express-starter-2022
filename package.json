{
  "name": "node-express-starter-2022",
  "version": "2.1.0",
  "description": "A node and express starter template for 2022",
  "author": "Carter Adams <carteradams@protonmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -r dist/* logs/*",
    "build": "babel src -d dist --copy-files",
    "dev": "babel-watch -w .env -w src src/index.js",
    "lint": "eslint src",
    "start": "node dist/index.js",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.4.0",
    "express-validator": "^6.14.0",
    "get-port": "^5.1.1",
    "helmet": "^5.0.2",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@types/jest": "^27.5.0",
    "babel-watch": "^7.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "regenerator-runtime": "^0.13.9",
    "supertest": "^6.2.3"
  }
}
